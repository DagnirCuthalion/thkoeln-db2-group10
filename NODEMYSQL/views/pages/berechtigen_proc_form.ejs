<%- include ("../partials/header") %>

<div class="row" style="margin-left: 5px;margin-right:5px">
    <form action="http://localhost:3000/berechtigen_proc" method ="post" class="col s12">
        
    
        <div class="form-group row">
            <label for="raumverantwortlicher_id" class="col-2 col-form-label">raumverantwortlicher ID</label>
            <div class="col-10">
                <input type="number" name="raumverantwortlicher_id" value="" class="form-control">
            </div>
        </div>
    
        <div class="form-group row">
            <label for="person_id" class="col-2 col-form-label">Person ID</label>
            <div class="col-10">
                <input type="number" name="person_id" value="" class="form-control">
            </div>
        </div>
    
        <div class="form-group row">
            <label for="raum_nr" class="col-2 col-form-label">RaumNR</label>
            <div class="col-10">
                <input type="number" name="raum_nr" value="" class="form-control">
            </div>
        </div>

        <div class="form-group row">
            <label for="berechtigt_von" class="col-2 col-form-label">berechtigt_von</label>
            <div class="col-10">
                <input type="date" name="berechtigt_von" value="" class="form-control">
            </div>
        </div>
    
        <div class="form-group row">
            <label for="berechtigt_bis" class="col-2 col-form-label">berechtigt bis</label>
            <div class="col-10">
                <input type="date" name="berechtigt_bis" value="" class="form-control">
            </div>
        </div>
    
        <div class="form-group row">
            <label for="e_location" class="col-2 col-form-label" style="color:#ffffff">Click
            here to submit</label>
            <div class="col-10">
                <input type="submit" name="submit" value="Berechtigen" class="btn btn-primary btn-lg">
            </div>
        </div>
    </form>
</div>


<%- include ("../partials/footer") %>