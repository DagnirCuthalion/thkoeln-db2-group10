<%- include ("../partials/header") %>

<div class="row" style="margin-left: 5px;margin-right:5px">
    <form action = "<%=dynIn.action%>" method="post" class="col s12">
        <% for (var row of dynIn.arr) { %>
        <div class="form-group row">
            <label for="<%= row.name %>" class="col-2 col-form-label"><%= row.name %></label>
            <div class="col-10">
                <input type="<%= typeof(row.value) %>" name="<%= row.name %>" value="" class="form-control">
            </div>
        </div>
        <%}%>
        <div class="form-group row">
            <label for="submit" class="col-2 col-form-label" style="color:#ffffff">'Click here to submit'</label>
            <div class="col-10">
                <input type="submit" name="submit" value="Save <%=dynIn.tablename%>" class="btn btn-primary btn-lg">
            </div>
        </div>
    </form>
</div>

<%- include ("../partials/footer") %>